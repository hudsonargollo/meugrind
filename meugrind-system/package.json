{
  "name": "meugrind-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:property": "jest --testNamePattern='Property' --verbose",
    "test:property:watch": "jest --testNamePattern='Property' --watch --verbose",
    "build:analyze": "ANALYZE=true npm run build",
    "export": "next export",
    "build:production": "next build",
    "deploy:build": "npm run lint && npm run test && npm run build:production",
    "wrangler:deploy": "wrangler pages deploy out --project-name=meugrind-system",
    "wrangler:deploy:production": "wrangler pages deploy out --project-name=meugrind-system --env=production",
    "wrangler:deploy:preview": "wrangler pages deploy out --project-name=meugrind-system --env=preview",
    "deploy:wrangler": "npm run deploy:build && npm run wrangler:deploy:production",
    "deploy:wrangler:preview": "npm run deploy:build && npm run wrangler:deploy:preview",
    "deploy": "node deploy.js",
    "deploy:preview": "node deploy.js --preview",
    "deploy:fast": "node deploy.js --skip-tests",
    "deploy:build-only": "node deploy.js --skip-build"
  },
  "dependencies": {
    "@aws-amplify/auth": "^6.17.1",
    "@aws-amplify/ui-react": "^6.13.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.16",
    "@types/uuid": "^10.0.0",
    "aws-amplify": "^6.15.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "framer-motion": "^12.26.2",
    "lucide-react": "^0.562.0",
    "next": "14.2.35",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "fake-indexeddb": "^6.2.5",
    "fast-check": "^4.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5"
  }
}
